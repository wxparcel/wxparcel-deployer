{
  "name": "wxparcel-deployer",
  "version": "1.0.0-alpha.1.2",
  "bin": "./bin/cli",
  "main": "./libs/index.js",
  "license": "MIT",
  "description": "小程序发布服务",
  "author": "DavidJones<qowera@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxparcel/wxparcel-deployer.git"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "tslint": "tslint --config ./tslint.json --fix './src/**/*.ts'",
    "lint": "npm run tslint",
    "test": "npm run lint",
    "compile": "tsc --outDir ./libs ./src/*.ts --target es5",
    "server": "node ./libs/cli.js server",
    "develop": "nodemon --watch src --ext ts --exec \"npm run compile && npm run server\"",
    "release": "rimraf ./libs && npm run compile",
    "start": "npm run develop"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/form-data": "^2.2.1",
    "@types/formidable": "^1.0.31",
    "@types/fs-extra": "^5.0.5",
    "@types/ip": "^1.1.0",
    "@types/jszip": "^3.1.5",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.10.5",
    "@types/path-to-regexp": "^1.7.0",
    "@types/portscanner": "^2.1.0",
    "@types/socket.io": "^2.1.2",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.10",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@types/shortid": "^0.0.29",
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "cli-progress": "^2.1.1",
    "command-exists": "^1.2.8",
    "commander": "^2.19.0",
    "form-data": "^2.3.3",
    "formidable": "^1.2.1",
    "fs-extra": "^7.0.1",
    "ip": "^1.1.5",
    "jszip": "^3.2.1",
    "lodash": "^4.17.11",
    "path-to-regexp": "^3.0.0",
    "portscanner": "^2.2.0",
    "pretty-error": "^2.1.1",
    "shortid": "^2.2.14",
    "socket.io": "^2.2.0"
  }
}
